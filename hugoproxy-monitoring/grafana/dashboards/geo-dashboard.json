{
  "title": "Geo Service Dashboard",
  "tags": ["geo", "service", "monitoring"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "HTTP Requests Rate",
      "type": "graph",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "{{method}} {{path}}",
          "refId": "A"
        }
      ],
      "xaxis": {"show": true},
      "yaxis": {"format": "reqps", "logBase": 1},
      "lines": true,
      "fill": 1
    },
    {
      "id": 2,
      "title": "HTTP Request Duration (95th percentile)",
      "type": "graph",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "{{method}} {{path}}",
          "refId": "A"
        }
      ],
      "xaxis": {"show": true},
      "yaxis": {"format": "s", "logBase": 1},
      "lines": true,
      "fill": 1
    },
    {
      "id": 3,
      "title": "Cache Hit Rate",
      "type": "graph",
      "gridPos": {"x": 0, "y": 8, "w": 8, "h": 8},
      "targets": [
        {
          "expr": "rate(cache_requests_total{cache_hit=\"true\"}[5m]) / rate(cache_requests_total[5m])",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "xaxis": {"show": true},
      "yaxis": {"format": "percent", "logBase": 1},
      "lines": true,
      "fill": 1
    },
    {
      "id": 4,
      "title": "External API Response Time",
      "type": "graph",
      "gridPos": {"x": 8, "y": 8, "w": 8, "h": 8},
      "targets": [
        {
          "expr": "rate(external_api_request_duration_seconds_sum[5m]) / rate(external_api_request_duration_seconds_count[5m])",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "xaxis": {"show": true},
      "yaxis": {"format": "s", "logBase": 1},
      "lines": true,
      "fill": 1
    },
    {
      "id": 5,
      "title": "Database Query Time",
      "type": "graph",
      "gridPos": {"x": 16, "y": 8, "w": 8, "h": 8},
      "targets": [
        {
          "expr": "rate(db_request_duration_seconds_sum[5m]) / rate(db_request_duration_seconds_count[5m])",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "xaxis": {"show": true},
      "yaxis": {"format": "s", "logBase": 1},
      "lines": true,
      "fill": 1
    },
    {
      "id": 6,
      "title": "HTTP Status Codes",
      "type": "piechart",
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum by (status_code) (rate(http_requests_total[5m]))",
          "legendFormat": "{{status_code}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 7,
      "title": "Active Goroutines",
      "type": "graph",
      "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "go_goroutines",
          "legendFormat": "instance: {{instance}}",
          "refId": "A"
        }
      ],
      "xaxis": {"show": true},
      "yaxis": {"format": "short", "logBase": 1},
      "lines": true,
      "fill": 1
    }
  ],
  "time": {"from": "now-6h", "to": "now"},
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "templating": {
    "list": [
      {
        "name": "instance",
        "type": "query",
        "query": "label_values(up, instance)",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      }
    ]
  },
  "refresh": "10s",
  "schemaVersion": 27,
  "version": 1,
  "links": [
    {
      "icon": "external link",
      "tags": [],
      "type": "link",
      "name": "Prometheus",
      "url": "http://localhost:9090"
    },
    {
      "icon": "external link",
      "tags": [],
      "type": "link",
      "name": "Geo Service",
      "url": "http://localhost:8080"
    }
  ]
}